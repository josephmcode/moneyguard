    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F6F7F8"
      },
      "package": "com.moneyguard.beta"
    },
    "plugins": []
  }
}