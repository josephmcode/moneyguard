{
  "expo": {
    "name": "MoneyGuard Beta",
    "slug": "moneyguard-beta",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "sdkVersion": "54.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F7F8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
